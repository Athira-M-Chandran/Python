    "from collections import Counter\n",
    "\n",
    "if __name__ == '__main__':\n",
    "\n",
    "    X = int(input())\n",
    "    size = Counter(map(int,input().split()))\n",
    "    C = int(input())\n",
    "    earning = 0\n",
    "    \n",
    "    for _ in range(C):\n",
    "        customer_size, price = map(int, input().split())\n",
    "        \n",
    "        if customer_size in size and size[customer_size] >0:\n",
    "            size[customer_size] -= 1\n",
    "            earning +=price\n",
    "            \n",
    "    print(earning)"
   ]
